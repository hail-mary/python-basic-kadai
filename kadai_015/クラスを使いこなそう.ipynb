        "class Human:\n",
        "  def __init__(self, name: str, age: int):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def printinfo(self):\n",
        "    print(f'name: {self.name}, age: {self.age}')\n",
        "\n",
        "person = Human('John', 25)\n",
        "person.printinfo()"
      ]
